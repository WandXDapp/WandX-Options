{
  "name": "wandx-options",
  "version": "0.2.3",
  "description": "Repository to facilitate the options feature for trading ERC20 token using wandx protocol",
  "main": "truffle.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "should": "13.1.3",
    "sol-merger": "^0.1.2",
    "solidity-coverage": "0.4.7",
    "ganache-cli": "^6.1.0",
    "solc": "0.4.18",
    "truffle": "4.1.7"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "babel-register": "6.26.0",
    "bignumber.js": "4.0.2",
    "codecov": "2.3.0",
    "ether-pudding": "^3.2.0",
    "ethereumjs-testrpc": "4.1.1",
    "ethereumjs-tx": "^1.3.4",
    "fs": "0.0.1-security",
    "lodash": "4.17.4",
    "mocha-lcov-reporter": "1.3.0",
    "solc": "0.4.19",
    "truffle": "4.0.1",
    "truffle-hdwallet-provider": "^0.0.3",
    "truffle-wallet-provider": "^0.0.5",
    "web3": "^1.0.0-beta.30",
    "xtend": "4.0.1",
    "yargs": "8.0.2",
    "zeppelin-solidity": "1.7.0"
  },
  "scripts": {
    "test": "scripts/test.sh",
    "ganache-cli": "node_modules/.bin/ganache-cli -i 15 --gasLimit 50000000000",
    "compile": "truffle compile --optimize-runs 200",
    "coverage": "./node_modules/.bin/solidity-coverage",
    "flatten": "sol-merger './contracts/*.sol' ./flat",
    "flatten-storage": "sol-merger './contracts/storage/*.sol' ./flat/flatStorage",
    "migrate-dev": "truffle migrate --network=development --reset --all",
    "migrate-ropsten": "truffle migrate --network=ropsten --reset -all",
    "migrate-main": "truffle migrate --network=main",
    "flatten-dummytoken": "sol-merger './test/mock_contracts/*.sol' ./test/mock_contracts/flat",
    "compile-dummytoken": "npm run flatten-dummytoken && node ./test/helpers/compile_dummytoken"
  },
  "keywords": [
    "call",
    "put"
  ],
  "author": "Satyam Agrawal",
  "license": "ISC"
}
